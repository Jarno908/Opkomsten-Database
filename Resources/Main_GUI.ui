<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="ttk.Notebook" id="Main_Notebook">
    <property name="height">500</property>
    <property name="width">800</property>
    <layout>
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
      <property name="sticky">nsew</property>
    </layout>
    <child>
      <object class="ttk.Notebook.Tab" id="Tab_FileSearcher">
        <property name="text" translatable="yes">Zoeken</property>
        <child>
          <object class="ttk.Frame" id="Frame_FileSearcher">
            <property name="height">200</property>
            <property name="width">200</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="sticky">nsew</property>
              <rows>
                <row id="0">
                  <property name="weight">0</property>
                </row>
                <row id="1">
                  <property name="weight">1</property>
                </row>
              </rows>
              <columns>
                <column id="0">
                  <property name="weight">1</property>
                </column>
              </columns>
            </layout>
            <child>
              <object class="ttk.Notebook" id="FileSearcher_Notebook">
                <property name="height">200</property>
                <property name="width">200</property>
                <layout>
                  <property name="column">0</property>
                  <property name="propagate">True</property>
                  <property name="row">1</property>
                  <property name="sticky">nsew</property>
                </layout>
                <child>
                  <object class="ttk.Notebook.Tab" id="Opkomst_tab">
                    <property name="sticky">nsew</property>
                    <property name="text" translatable="yes">Opkomsten</property>
                    <child>
                      <object class="ttk.Frame" id="Frame_Opkomst">
                        <property name="height">200</property>
                        <property name="width">200</property>
                        <layout>
                          <property name="column">0</property>
                          <property name="propagate">True</property>
                          <property name="row">0</property>
                          <property name="sticky">nsew</property>
                          <rows>
                            <row id="2">
                              <property name="weight">0</property>
                            </row>
                            <row id="3">
                              <property name="weight">1</property>
                            </row>
                            <row id="4">
                              <property name="weight">1</property>
                            </row>
                            <row id="5">
                              <property name="weight">1</property>
                            </row>
                            <row id="6">
                              <property name="weight">2</property>
                            </row>
                          </rows>
                          <columns>
                            <column id="0">
                              <property name="weight">1</property>
                            </column>
                            <column id="1">
                              <property name="weight">6</property>
                            </column>
                          </columns>
                        </layout>
                        <child>
                          <object class="ttk.Label" id="Opkomsten_Title">
                            <property name="font">{DejaVu Sans} 14 {bold}</property>
                            <property name="justify">center</property>
                            <property name="text" translatable="yes">Hier kun je naar opkomsten zoeken.</property>
                            <layout>
                              <property name="column">0</property>
                              <property name="columnspan">2</property>
                              <property name="pady">5</property>
                              <property name="propagate">True</property>
                              <property name="row">0</property>
                              <property name="sticky">n</property>
                            </layout>
                          </object>
                        </child>
                        <child>
                          <object class="ttk.Label" id="Opkomsten_Description">
                            <property name="text" translatable="yes">Hier kun je opkomsten opzoeken.</property>
                            <layout>
                              <property name="column">0</property>
                              <property name="columnspan">2</property>
                              <property name="pady">5</property>
                              <property name="propagate">True</property>
                              <property name="row">1</property>
                            </layout>
                          </object>
                        </child>
                        <child>
                          <object class="ttk.Separator" id="Separator_4">
                            <property name="orient">horizontal</property>
                            <layout>
                              <property name="column">0</property>
                              <property name="columnspan">2</property>
                              <property name="propagate">True</property>
                              <property name="row">2</property>
                              <property name="sticky">new</property>
                            </layout>
                          </object>
                        </child>
                        <child>
                          <object class="ttk.Label" id="Opkomsten_Searchwords_Label">
                            <property name="text" translatable="yes">Zoeken:</property>
                            <layout>
                              <property name="column">0</property>
                              <property name="propagate">True</property>
                              <property name="row">3</property>
                              <property name="sticky">e</property>
                            </layout>
                          </object>
                        </child>
                        <child>
                          <object class="ttk.Entry" id="Opkomsten_Searchwords_Entry">
                            <property name="justify">left</property>
                            <property name="state">normal</property>
                            <layout>
                              <property name="column">1</property>
                              <property name="padx">50</property>
                              <property name="propagate">True</property>
                              <property name="row">3</property>
                              <property name="sticky">ew</property>
                            </layout>
                          </object>
                        </child>
                        <child>
                          <object class="ttk.Label" id="Opkomsten_Speltakken_Label">
                            <property name="text" translatable="yes">Speltak:</property>
                            <layout>
                              <property name="column">0</property>
                              <property name="propagate">True</property>
                              <property name="row">4</property>
                              <property name="sticky">e</property>
                            </layout>
                          </object>
                        </child>
                        <child>
                          <object class="ttk.Combobox" id="Opkomsten_Speltakken_Combobox">
                            <property name="validate">none</property>
                            <property name="values">Alle Bevers Welpen Verkenners Explorers Roverscouts Stam +Scouts Overig</property>
                            <layout>
                              <property name="column">1</property>
                              <property name="padx">50</property>
                              <property name="propagate">True</property>
                              <property name="row">4</property>
                              <property name="sticky">w</property>
                            </layout>
                          </object>
                        </child>
                        <child>
                          <object class="ttk.Label" id="Opkomsten_Categories_Label">
                            <property name="text" translatable="yes">Spelgebied:</property>
                            <layout>
                              <property name="column">0</property>
                              <property name="propagate">True</property>
                              <property name="row">5</property>
                              <property name="sticky">e</property>
                            </layout>
                          </object>
                        </child>
                        <child>
                          <object class="ttk.Combobox" id="Opkomsten_Categories_Combobox">
                            <property name="values">Alle Buitenleven Expressie Identiteit Internationaal Samenleving "Sport &amp; Spel" "Uitdagende Scoutingtechnieken" "Veilig &amp; Gezond" Overig</property>
                            <layout>
                              <property name="column">1</property>
                              <property name="padx">50</property>
                              <property name="propagate">True</property>
                              <property name="row">5</property>
                              <property name="sticky">w</property>
                            </layout>
                          </object>
                        </child>
                        <child>
                          <object class="ttk.Button" id="Opkomsten_Button">
                            <property name="command">opkomsten_button_pressed</property>
                            <property name="text" translatable="yes">Zoeken</property>
                            <layout>
                              <property name="column">0</property>
                              <property name="columnspan">2</property>
                              <property name="ipadx">40</property>
                              <property name="ipady">20</property>
                              <property name="propagate">True</property>
                              <property name="row">6</property>
                            </layout>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="ttk.Separator" id="Separator_5">
                <property name="orient">horizontal</property>
                <layout>
                  <property name="column">0</property>
                  <property name="pady">2</property>
                  <property name="propagate">True</property>
                  <property name="row">0</property>
                  <property name="sticky">ew</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Notebook.Tab" id="Tab_FileUpload">
        <property name="text" translatable="yes">Uploaden</property>
        <child>
          <object class="ttk.Frame" id="Frame_FileUpload">
            <property name="height">200</property>
            <property name="width">200</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="sticky">nsew</property>
              <rows>
                <row id="0">
                  <property name="weight">0</property>
                </row>
                <row id="1">
                  <property name="weight">0</property>
                </row>
                <row id="2">
                  <property name="weight">0</property>
                </row>
                <row id="3">
                  <property name="weight">10</property>
                </row>
                <row id="4">
                  <property name="weight">10</property>
                </row>
                <row id="5">
                  <property name="weight">10</property>
                </row>
              </rows>
              <columns>
                <column id="0">
                  <property name="weight">1</property>
                </column>
              </columns>
            </layout>
            <child>
              <object class="ttk.Label" id="Upload_Title">
                <property name="cursor">based_arrow_down</property>
                <property name="font">{DejaVu Sans} 14 {bold}</property>
                <property name="text" translatable="yes">Documenten Uploaden</property>
                <layout>
                  <property name="column">0</property>
                  <property name="pady">5</property>
                  <property name="propagate">True</property>
                  <property name="row">0</property>
                  <property name="sticky">n</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Label" id="Upload_Description">
                <property name="justify">center</property>
                <property name="text" translatable="yes">Selecteer de documenten die je wilt uploaden. 
Vervolgens klik op "Uploaden" om de documenten te uploaden.</property>
                <property name="wraplength">700</property>
                <layout>
                  <property name="column">0</property>
                  <property name="pady">5</property>
                  <property name="propagate">True</property>
                  <property name="row">1</property>
                  <property name="sticky">n</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Separator" id="Separator_1">
                <property name="orient">horizontal</property>
                <layout>
                  <property name="column">0</property>
                  <property name="columnspan">1</property>
                  <property name="propagate">True</property>
                  <property name="row">2</property>
                  <property name="sticky">new</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="customappwidgets.customPathChooserWidget" id="Pathchooser_Upload">
                <property name="selection type">files</property>
                <property name="title" translatable="yes">Selecteer een of meerdere bestanden.</property>
                <bind add="" handler="upload_path_changed" sequence="&lt;&lt;PathChooserPathChanged&gt;&gt;" />
                <layout>
                  <property name="column">0</property>
                  <property name="padx">40</property>
                  <property name="propagate">True</property>
                  <property name="row">3</property>
                  <property name="sticky">sew</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="Button_Upload">
                <property name="command">upload_button_pressed</property>
                <property name="state">disabled</property>
                <property name="text" translatable="yes">Uploaden</property>
                <layout>
                  <property name="column">0</property>
                  <property name="ipadx">10</property>
                  <property name="ipady">10</property>
                  <property name="pady">5</property>
                  <property name="propagate">True</property>
                  <property name="row">4</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Label" id="Upload_Logo">
                <layout>
                  <property name="column">0</property>
                  <property name="propagate">True</property>
                  <property name="row">5</property>
                  <property name="sticky">n</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Notebook.Tab" id="Tab_UpdateFile">
        <property name="text" translatable="yes">Update</property>
        <child>
          <object class="ttk.Frame" id="Frame_UpdateFile">
            <property name="height">200</property>
            <property name="width">200</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="sticky">nsew</property>
              <rows>
                <row id="0">
                  <property name="weight">0</property>
                </row>
                <row id="1">
                  <property name="weight">0</property>
                </row>
                <row id="2">
                  <property name="weight">0</property>
                </row>
                <row id="3">
                  <property name="weight">10</property>
                </row>
                <row id="4">
                  <property name="weight">10</property>
                </row>
                <row id="5">
                  <property name="weight">10</property>
                </row>
              </rows>
              <columns>
                <column id="0">
                  <property name="weight">1</property>
                </column>
              </columns>
            </layout>
            <child>
              <object class="ttk.Label" id="Update_Title">
                <property name="cursor">based_arrow_down</property>
                <property name="font">{DejaVu Sans} 14 {bold}</property>
                <property name="text" translatable="yes">Documenten Updaten</property>
                <layout>
                  <property name="column">0</property>
                  <property name="pady">5</property>
                  <property name="propagate">True</property>
                  <property name="row">0</property>
                  <property name="sticky">n</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Label" id="Update_Description">
                <property name="justify">center</property>
                <property name="text" translatable="yes">Hier kun je een document dat je al eerder hebt geupload updaten naar een nieuwere versie.
(De vorige oude versie wordt gedelete!!!)</property>
                <property name="wraplength">700</property>
                <layout>
                  <property name="column">0</property>
                  <property name="pady">5</property>
                  <property name="propagate">True</property>
                  <property name="row">1</property>
                  <property name="sticky">n</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Separator" id="Separator_2">
                <property name="orient">horizontal</property>
                <layout>
                  <property name="column">0</property>
                  <property name="columnspan">1</property>
                  <property name="propagate">True</property>
                  <property name="row">2</property>
                  <property name="sticky">new</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="customappwidgets.customPathChooserWidget" id="Pathchooser_Update">
                <property name="selection type">file</property>
                <property name="title" translatable="yes">Selecteer een of meerdere bestanden.</property>
                <bind add="" handler="update_path_changed" sequence="&lt;&lt;PathChooserPathChanged&gt;&gt;" />
                <layout>
                  <property name="column">0</property>
                  <property name="padx">40</property>
                  <property name="propagate">True</property>
                  <property name="row">3</property>
                  <property name="sticky">sew</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="Button_Update">
                <property name="command">update_button_pressed</property>
                <property name="state">disabled</property>
                <property name="text" translatable="yes">Update</property>
                <layout>
                  <property name="column">0</property>
                  <property name="ipadx">10</property>
                  <property name="ipady">10</property>
                  <property name="pady">5</property>
                  <property name="propagate">True</property>
                  <property name="row">4</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Label" id="Update_Logo">
                <layout>
                  <property name="column">0</property>
                  <property name="propagate">True</property>
                  <property name="row">5</property>
                  <property name="sticky">n</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Notebook.Tab" id="Tab_Settings">
        <property name="text" translatable="yes">Instellingen</property>
        <child>
          <object class="ttk.Frame" id="Frame_Settings">
            <property name="height">200</property>
            <property name="width">200</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="sticky">nsew</property>
              <rows>
                <row id="0">
                  <property name="weight">0</property>
                </row>
                <row id="1">
                  <property name="weight">0</property>
                </row>
                <row id="2">
                  <property name="weight">0</property>
                </row>
                <row id="3">
                  <property name="weight">0</property>
                </row>
              </rows>
              <columns>
                <column id="0">
                  <property name="weight">1</property>
                </column>
                <column id="1">
                  <property name="weight">1</property>
                </column>
                <column id="2">
                  <property name="weight">1</property>
                </column>
              </columns>
            </layout>
            <child>
              <object class="ttk.Label" id="Settings_Title">
                <property name="font">{DejaVu Sans} 14 {bold}</property>
                <property name="text" translatable="yes">Instellingen</property>
                <layout>
                  <property name="column">0</property>
                  <property name="columnspan">3</property>
                  <property name="pady">5</property>
                  <property name="propagate">True</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Label" id="Settings_Description">
                <property name="text" translatable="yes">HIer kun je verschillende instellingen aanpassen.</property>
                <layout>
                  <property name="column">0</property>
                  <property name="columnspan">3</property>
                  <property name="pady">5</property>
                  <property name="propagate">True</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Separator" id="Settings_Seperator">
                <property name="orient">horizontal</property>
                <layout>
                  <property name="column">0</property>
                  <property name="columnspan">3</property>
                  <property name="propagate">True</property>
                  <property name="row">2</property>
                  <property name="sticky">ew</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Label" id="Upload_Name_Label">
                <property name="text" translatable="yes">Uploader Naam:</property>
                <layout>
                  <property name="column">0</property>
                  <property name="pady">10</property>
                  <property name="propagate">True</property>
                  <property name="row">3</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Entry" id="Upload_Name_Entry">
                <property name="state">normal</property>
                <property name="textvariable">string:uploader_name_text</property>
                <layout>
                  <property name="column">1</property>
                  <property name="ipadx">50</property>
                  <property name="propagate">True</property>
                  <property name="row">3</property>
                  <property name="sticky">w</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="Upload_Name_Button">
                <property name="command">update_uploader_name</property>
                <property name="text" translatable="yes">Verander Naam</property>
                <layout>
                  <property name="column">2</property>
                  <property name="ipadx">20</property>
                  <property name="propagate">True</property>
                  <property name="row">3</property>
                  <property name="sticky">w</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Label" id="Download_Location_Label">
                <property name="text" translatable="yes">Download folder:</property>
                <layout>
                  <property name="column">0</property>
                  <property name="propagate">True</property>
                  <property name="row">4</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="customappwidgets.customPathChooserWidget" id="Pathchooser_Download">
                <property name="selection type">directory</property>
                <bind add="" handler="download_path_changed" sequence="&lt;&lt;PathChooserPathChanged&gt;&gt;" />
                <layout>
                  <property name="column">1</property>
                  <property name="columnspan">2</property>
                  <property name="ipadx">50</property>
                  <property name="propagate">True</property>
                  <property name="row">4</property>
                  <property name="sticky">w</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
